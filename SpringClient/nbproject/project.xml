                    <file>${file.reference.joda-time-2.2.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.hibernate-validator-5.0.1.Final.jar}</file>
